#!/usr/bin/env python

Import('env')

root_packages = [
    env.Go('fmath.go'),
    env.Go('logging.go'),
    env.Go('time.go'),
    env.Go('stack', Glob('stack/*.go')),
]

subpackages = SConscript(['core/SConscript', 'std/SConscript'], exports='env')
packages = Flatten([root_packages, subpackages])

Return('packages')
